<%= form_with url: index_path, local: true, method: :get, html: { class: "navbar-form navbar-left fix-form-position" } do |form| %>
  <%= form.text_field :q, placeholder: t("layouts.header.search"), data: { behavior: "autocomplete" }, class: "form-control search-input" %>
  <%= form.button t("search"), class: "btn btn-default" %>
<% end %>
